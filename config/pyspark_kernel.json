{
 "display_name": "PySpark",
 "language": "python",
 "argv": [
  "/opt/anaconda/bin/python",
  "-m",
  "ipykernel",
  "-f",
  "{connection_file}"
 ],
 "env": {
  "JAVA_HOME": "/usr/lib/jvm/java-1.8.0",
  "SPARK_HOME": "/usr/lib/spark",
  "HADOOP_CONF_DIR": "/etc/hadoop/conf",
  "PYTHONPATH": " /usr/lib/spark/python/:/usr/lib/spark/python/lib/py4j-0.10.4-src.zip ",
  "PYTHONSTARTUP": "/usr/lib/spark/python/pyspark/shell.py",
  "PYSPARK_PYTHON": "/opt/anaconda/bin/python",
  "PYSPARK_SUBMIT_ARGS": "--conf spark.port.maxRetries=35 --master yarn-client pyspark-shell"
 }
}

